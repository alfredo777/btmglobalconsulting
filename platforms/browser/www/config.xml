<?xml version='1.0' encoding='utf-8'?>
<widget id="btmconsultin.heroku.com" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>BTM Consulting Calendar</name>
    <description>
       App de  BTM Consulting
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        Rockstars MX
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="facebook.com" subdomains="true" />
    <access origin="*.fbcdn.net" />
    <access origin="*.akamaihd.net" />
    <access origin="*://*.googleapis.com/*" subdomains="true" />
    <access origin="*://*.gstatic.com/*" subdomains="true" />
    <access origin="*://*.google.com/*" subdomains="true" />
    <access origin="*://*.googleusercontent.com/*" subdomains="true" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto://*" />
    <allow-intent href="geo://*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
